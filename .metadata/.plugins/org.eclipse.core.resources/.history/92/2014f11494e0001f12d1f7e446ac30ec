package basic;

	class amsterdam1{
		//Total words in ur string
		//Occurence of each words
		public int countAm1(String s) {
			String str = s.toLowerCase();
//			
			int cnt=0;
			int cnt1=1;
			String a[] = str.split(" ");
			for (int i = 0; i < a.length; i++) {
				//System.out.println(a[i]);
				for(int j = 1;j< a.length-1;j++) {
					if(a[i]==a[j]) {
						cnt=cnt+1;
						if(cnt>2) {
							cnt1=cnt1+1;
							System.out.println(i+"--"+a[i]);
						}
						//System.out.println(a[i]);
						
					}//System.out.println(a[i]+a[j]);
					
				}
				
				/*if (a[i].equals("is")) {
					cnt = cnt + 1;
				}
				//System.out.println(i+"--"+a[i]);
			*/
			}
			return cnt;
		}
	}

public class string2 {

	
			public static void main(String[] args) {
				
				amsterdam1 a1 = new amsterdam1();
				//int count =a1.countAm(s);
				int count =a1.countAm1("I learn java from EB. Java is most popular programming language. Java is platform independent. Java is secure");
				System.out.println(count);
				//System.out.println("Print Count :"+count);


			}


		
	}